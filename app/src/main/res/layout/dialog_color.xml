<?xml version="1.0" encoding="utf-8"?><!--
style/OurThemeName - disable  ON/OFF com.walhalla.pillfinder.ui.custom.SquareToggleButton
 -->
<FrameLayout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="@android:color/background_light"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <!--android:background="@drawable/ic_dialog_color_bg"-->
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_margin="0dp"

        android:gravity="center"
        android:orientation="vertical"
        tools:ignore="UselessParent">

        <!--<View-->
        <!--android:layout_width="match_parent"-->
        <!--android:layout_height="2dp"-->
        <!--android:layout_marginStart="32dp"-->
        <!--android:layout_marginLeft="32dp"-->
        <!--android:layout_marginEnd="32dp"-->
        <!--android:layout_marginRight="32dp"-->
        <!--android:background="@color/white" />-->

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@android:color/white"
            android:drawableStart="@drawable/ic_color"
            android:drawableLeft="@drawable/ic_color"
            android:drawablePadding="10dp"
            android:padding="@dimen/activity_horizontal_margin"
            android:text="@string/dialog_color_title"
            android:textAppearance="@style/TextAppearance.AppCompat.Large"
            android:textColor="@color/black"
            android:textStyle="bold" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@null"
            android:gravity="center"
            android:orientation="vertical"
            android:padding="@dimen/activity_horizontal_margin">

            <LinearLayout
                android:id="@+id/line1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <com.walhalla.pillfinder.ui.custom.SquareToggleButton
                    android:id="@+id/ib0"
                    style="@style/OurThemeName"
                    android:layout_height="wrap_content"
                    android:background="@drawable/btn_toggle_bg_black"
                    android:contentDescription="@string/paint"
                    android:tag="BLACK" />

                <com.walhalla.pillfinder.ui.custom.SquareToggleButton
                    android:id="@+id/ib1"
                    style="@style/OurThemeName"
                    android:background="@drawable/btn_toggle_bg_blue"
                    android:contentDescription="@string/paint"
                    android:tag="BLUE" />

                <com.walhalla.pillfinder.ui.custom.SquareToggleButton
                    android:id="@+id/ib2"
                    style="@style/OurThemeName"
                    android:background="@drawable/btn_toggle_bg_brown"
                    android:contentDescription="@string/paint"
                    android:tag="BROWN" />

                <com.walhalla.pillfinder.ui.custom.SquareToggleButton
                    android:id="@+id/ib3"
                    style="@style/OurThemeName"
                    android:background="@drawable/btn_toggle_bg_gray"
                    android:contentDescription="@string/paint"
                    android:tag="GRAY" />


            </LinearLayout>

            <LinearLayout
                android:id="@+id/line2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <com.walhalla.pillfinder.ui.custom.SquareToggleButton
                    android:id="@+id/ib4"
                    style="@style/OurThemeName"
                    android:background="@drawable/btn_toggle_bg_green"
                    android:contentDescription="@string/paint"
                    android:tag="GREEN" />

                <com.walhalla.pillfinder.ui.custom.SquareToggleButton
                    android:id="@+id/ib5"
                    style="@style/OurThemeName"
                    android:background="@drawable/btn_toggle_bg_orange"
                    android:contentDescription="@string/paint"
                    android:tag="ORANGE" />

                <com.walhalla.pillfinder.ui.custom.SquareToggleButton
                    android:id="@+id/ib6"
                    style="@style/OurThemeName"
                    android:background="@drawable/btn_toggle_bg_pink"
                    android:contentDescription="@string/paint"
                    android:tag="PINK" />

                <com.walhalla.pillfinder.ui.custom.SquareToggleButton
                    android:id="@+id/ib7"
                    style="@style/OurThemeName"
                    android:background="@drawable/btn_toggle_bg_purple"
                    android:contentDescription="@string/paint"
                    android:tag="PURPLE" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/line3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">


                <com.walhalla.pillfinder.ui.custom.SquareToggleButton
                    android:id="@+id/ib8"
                    style="@style/OurThemeName"
                    android:background="@drawable/btn_toggle_bg_red"
                    android:contentDescription="@string/paint"
                    android:tag="RED" />

                <com.walhalla.pillfinder.ui.custom.SquareToggleButton
                    android:id="@+id/ib9"
                    style="@style/OurThemeName"
                    android:background="@drawable/btn_toggle_bg_turquoise"
                    android:contentDescription="@string/paint"
                    android:tag="TURQUOISE" />

                <com.walhalla.pillfinder.ui.custom.SquareToggleButton
                    android:id="@+id/ib10"
                    style="@style/OurThemeName"
                    android:background="@drawable/btn_toggle_bg_white"
                    android:contentDescription="@string/paint"
                    android:tag="WHITE" />

                <com.walhalla.pillfinder.ui.custom.SquareToggleButton
                    android:id="@+id/ib11"
                    style="@style/OurThemeName"
                    android:background="@drawable/btn_toggle_bg_yellow"
                    android:contentDescription="@string/paint"
                    android:tag="YELLOW" />

            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:background="@null"
            android:gravity="end"
            android:orientation="horizontal"
            android:paddingLeft="@dimen/activity_horizontal_margin"
            android:paddingEnd="@dimen/activity_horizontal_margin"
            android:paddingRight="@dimen/activity_horizontal_margin">

            <Button
                android:id="@+id/no"
                style="?attr/buttonBarButtonStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@null"
                android:clickable="true"
                android:focusable="true"
                android:padding="@dimen/activity_horizontal_margin"
                android:text="@android:string/cancel"
                android:textColor="@color/black"
                android:textStyle="bold" />

            <Button
                android:id="@+id/yes11"
                style="?attr/buttonBarButtonStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@null"
                android:clickable="true"
                android:focusable="true"
                android:padding="@dimen/activity_horizontal_margin"
                android:text="@android:string/ok"
                android:textColor="@color/black"
                android:textStyle="bold" />


        </LinearLayout>

        <!--<View-->
        <!--android:layout_width="match_parent"-->
        <!--android:layout_height="2dp"-->
        <!--android:layout_marginStart="32dp"-->
        <!--android:layout_marginLeft="32dp"-->
        <!--android:layout_marginEnd="32dp"-->
        <!--android:layout_marginRight="32dp"-->
        <!--android:background="@color/white" />-->
    </LinearLayout>
</FrameLayout>